{
  "expo": {
    "name": "Avisos Comunidade",
    "slug": "avisos-comunidade",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#3498db"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#3498db"
      },
      "package": "com.avisoscomunidade.app"
    },
    "ios": {
      "bundleIdentifier": "com.avisoscomunidade.app",
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Um aplicativo para comunicação comunitária de eventos, feiras ou perigos locais.",
    "plugins": [
      "expo-router"
    ],
    "scheme": "avisos-comunidade"
  }
}
